<form id="submitForm" action="/submit" method="post">
    <input hidden id="shuDuArray" name="shuDuArray" value="{{ . }}">
</form>
<button id="shuDuBtn" onclick="submit()">submit</button>
</body>
<script>
    window.onload=function(){
        if (document.getElementById('shuDuArray').value !== "") {
            document.getElementById('shuDuBtn').remove()
            let array = document.getElementById('shuDuArray').value.split(",")
            let inputs, index;
            inputs = document.getElementsByTagName('input');
            for (index = 0; index < inputs.length - 1; ++index) {
                inputs[index].value = array[index]
            }
        }
    }
    function submit() {
        let inputs, index;
        inputs = document.getElementsByTagName('input');
        let arr = new Array(0)
        for (index = 0; index < inputs.length - 1; ++index) {
            if (inputs[index].value === "") {
                arr.push("0")
            } else {
                arr.push(inputs[index].value)
            }
        }
        document.getElementById('shuDuArray').value = arr;
        document.getElementById("submitForm").submit();
    }
</script>
